[%
my @nodes = $DB-&gt;getNodeWhere({author_user =&gt; getId($NODE)},
  "","",'light');
my $str;
foreach (@nodes) {
  next if getId($_) == getId($NODE);
  $str.="&lt;li&gt;".linkNode($_)."($$_{type}{title})&lt;/li&gt;\n";
}
$str = "Nodes authored by $$NODE{title}:&lt;ul&gt;".$str."&lt;/ul&gt;"
  ."&lt;br&gt;(".int(@nodes)." in total)" if $str;
$str ||= "&lt;i&gt;no nodes maintianed by $$NODE{title}&lt;/i&gt;\n";
%]

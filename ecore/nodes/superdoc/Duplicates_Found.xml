<NODE export_version="0.5" nodetype="superdoc" title="Duplicates Found">
  <field type="noderef" type_nodetype="user,nodetype" name="author_user">root</field>
  <field type="literal_value" name="authoraccess">iiii</field>
  <field type="literal_value" name="doctext">Multiple nodes named [%$query->param("node");%] were drunk&lt;br>

[%
	my $str;

	foreach my $NODE (@{ $$NODE{group} }) {
		$NODE = $DB->getNodeById($NODE, 'light');
		$str .= "&lt;LI>" . linkNode($NODE);
		$str .= "(type: $$NODE{type}{title})\n";
	}
	$str;
%]

</field>

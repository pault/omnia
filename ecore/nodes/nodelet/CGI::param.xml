my $str;
foreach ($query->param) {
  $str.="&lt;b>$_&lt;/b>:" . $query->param($_) . "&lt;br>\n" 
    unless length ($query->param($_)) > 30 or not defined $query->param($_);
}
$str;
%]</field>

	<nlcode table="nodelet">[%my $limit = 15;
$limit = int($$VARS{newnodes_limit}) if (exists $$VARS{newnodes_limit});

my $str;
my $csr = $DB-&gt;getDatabaseHandle()-&gt;prepare("
	SELECT * FROM node
	ORDER BY createtime DESC
	LIMIT $limit");

$csr-&gt;execute or return "SHIT";
while(my $N = $csr-&gt;fetchrow_hashref){
	$str .= linkNode($N) . "&lt;BR&gt;\n";	
}
$csr-&gt;finish;
$str;



%]</nlcode>

function cloneNode()
{
  var newname = prompt("Cloning node \"nodelet display page\"\nEnter a new name:", "");

  if(newname)
  {
    var url = "/~paco/index.pl?node_id=210&amp;srcnode_id=138&amp;newname=NEWNAME";
    
    url = url.replace(/NEWNAME/, newname);

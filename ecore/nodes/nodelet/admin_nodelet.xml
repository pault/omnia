&lt;script language="javascript"&gt;

function cloneNode()
{
  var newname = prompt("Cloning node \"admin nodelet\"\nEnter a new name:", "");

  if(newname)
  {
    var url = "/index.pl?node_id=166&amp;srcnode_id=158&amp;newname=NEWNAME";
    
    url = url.replace(/NEWNAME/, newname);
    url = url.replace(/ /g, "%20");
    parent.location = url;
  }
}


&lt;script language="javascript">

function cloneNode()
{
  var newname = prompt("Cloning node \"nodelet display page\"\nEnter a new name:", "");

  if(newname)
  {
    var url = "/~paco/index.pl?node_id=210&amp;srcnode_id=138&amp;newname=NEWNAME";
    
    url = url.replace(/NEWNAME/, newname);
    url = url.replace(/ /g, "%20");
    parent.location = url;
  }
}

&lt;/script>&lt;table width=100% border=0 cellpadding=3 bgcolor=#FFFFFF cellspacing=1>
         &lt;tr bgcolor=#ffffff> 

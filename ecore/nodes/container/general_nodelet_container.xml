  <field type="literal_value" name="groupaccess">iiii-</field>
  <field type="literal_value" name="guestaccess">iiii-</field>
  <field type="noderef" type_nodetype="location,nodetype" name="loc_location">container</field>
  <field type="literal_value" name="otheraccess">iiii-</field>
  <field type="noderef" type_nodetype="container,nodetype" name="parent_container">nodelet container</field>

  <field type="literal_value" name="authoraccess">iiii</field>
  <field type="literal_value" name="context">[%
	if (defined($$THEME{nodeletParent_container})) {
		$$CURRENTNODE{parent_container} = $$THEME{nodeletParent_container};
	} else {
		my $parent;
		if (defined($parent = $HTMLVARS{generalNodelet_container}) &amp;&amp;
		  $parent != $$CURRENTNODE{parent_container}) {
			$$CURRENTNODE{parent_container} = $parent;
		}
	}
"";
%]CONTAINED_STUFF</field>

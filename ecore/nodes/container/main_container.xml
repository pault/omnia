<NODE export_version="0.5" nodetype="container" title="main container">
  <field type_nodetype="user,nodetype" name="author_user" type="noderef">root</field>
  <field name="authoraccess" type="literal_value">iiii</field>
  <field name="context" type="literal_value">&lt;table width=95% align=center>
 &lt;tr>
  &lt;td>
   [{searchform:noendform}]
  &lt;/td>
  &lt;td align=right>
[%
my $default = getNode($HTMLVARS{default_node});
linkNode($default, "&lt;img border=0 src=images/e.gif>");
%]
   &lt;/td>
 &lt;/tr>
&lt;/form>
&lt;/table>

&lt;table width="100%">
 &lt;tr>  
  &lt;td width=100% valign=top>
   &lt;h3>[{showPath}] ["$$NODE{title}"]&lt;/h3>
   &lt;p>&lt;font size=2>Maintained by
   [%
   if ($$NODE{author_user} != getId ($USER)) {
	return linkNode ($$NODE{author_user});
   }
   linkNode($USER, "you"). "!!!&lt;br>"; 
   %]&lt;/font>
   &lt;p>
   CONTAINED_STUFF
  &lt;/td>
  &lt;td width=1 valign=top align=right>
   &lt;!-- nodelets start here -->
   [% $USER->isGod? "" : htmlcode('adminbar') %]
   [{nodelet meta-container}]
  &lt;/td>
 &lt;/tr>
&lt;/table></field>
  <field name="dynamicauthor_permission" type="literal_value">-1</field>
  <field name="dynamicgroup_permission" type="literal_value">-1</field>
  <field name="dynamicguest_permission" type="literal_value">-1</field>
  <field name="dynamicother_permission" type="literal_value">-1</field>
  <field name="group_usergroup" type="literal_value">-1</field>
  <field name="groupaccess" type="literal_value">iiii-</field>
  <field name="guestaccess" type="literal_value">iiii-</field>
  <field type_nodetype="location,nodetype" name="loc_location" type="noderef">container</field>
  <field name="otheraccess" type="literal_value">iiii-</field>
  <field type_nodetype="container,nodetype" name="parent_container" type="noderef">stdcontainer</field>
  <field name="title" type="literal_value">main container</field>
  <field type_nodetype="nodetype,nodetype" name="type_nodetype" type="noderef">container</field>
</NODE>

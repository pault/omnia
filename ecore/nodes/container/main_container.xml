<NODE export_version="0.5" nodetype="container" title="main container">
  <field name="author_user" type="noderef" type_nodetype="user,nodetype">root</field>
  <field name="authoraccess" type="literal_value">iiii</field>
  <field name="context" type="literal_value">&lt;div id=&quot;header&quot;&gt;
&lt;div id=&quot;searchform&quot;&gt;
   [{searchform}]
&lt;/div&gt;
&lt;div id=&quot;logoposition&quot;&gt;
[%
my $default = getNode($HTMLVARS{default_node});
my $IMG = getNode $$THEME{cornerIcon_image};
linkNode($default, &quot;&lt;img border=0 src=$$IMG{src} height=\&quot;48\&quot; width=\&quot;35\&quot; alt=\&quot;Corner Logo\&quot;\&gt;&quot;) if $IMG;
%]
&lt;/div&gt;
&lt;/div&gt;

&lt;div id=&quot;maincontainer&quot;&gt;
[%
my $page=&quot;&lt;div id=\&quot;containedstuff\&quot;&gt;

   &lt;h3&gt;&quot;.htmlcode('showPath').&quot;$$NODE{title}&lt;/h3&gt;
   &lt;p&gt;&lt;font size=2&gt;Maintained by &quot;.linkNode($$NODE{author_user}).&quot;&lt;/font&gt;
   &lt;p&gt;
   CONTAINED_STUFF&lt;/div&gt;&quot;;


my $nodelets= $page . &quot;&lt;span id=\&quot;nodelets\&quot;&gt;

   &lt;!-- nodelets start here --&gt;
   &quot;.htmlcode('nodeletMetaContainer');

  return $nodelets . &quot;&lt;/span&gt;&quot;;

%]
&lt;/div&gt;
</field>
  <field name="dynamicauthor_permission" type="literal_value">-1</field>
  <field name="dynamicgroup_permission" type="literal_value">-1</field>
  <field name="dynamicguest_permission" type="literal_value">-1</field>
  <field name="dynamicother_permission" type="literal_value">-1</field>
  <field name="group_usergroup" type="literal_value">-1</field>
  <field name="groupaccess" type="literal_value">iiii-</field>
  <field name="guestaccess" type="literal_value">iiii-</field>
  <field name="loc_location" type="noderef" type_nodetype="location,nodetype">container</field>
  <field name="otheraccess" type="literal_value">iiii-</field>
  <field name="parent_container" type="noderef" type_nodetype="container,nodetype">stdcontainer</field>
  <field name="title" type="literal_value">main container</field>
  <field name="type_nodetype" type="noderef" type_nodetype="nodetype,nodetype">container</field>
</NODE>

<NODE export_version="0.5" nodetype="container" title="suit nodelet container">
  <field name="author_user" type="noderef" type_nodetype="user,nodetype">root</field>
  <field name="authoraccess" type="literal_value">iiii</field>
  <field name="context" type="literal_value">&lt;table border=0 cellpadding=0 cellspacing=0 width=100%&gt;
&lt;tr&gt;&lt;td bgcolor=[%
   return $$THEME{nodeletTitleColor} if $$THEME{nodeletTitleColor};
    &quot;#CC99FF&quot;;
  %]&gt;
  [% miniNodelet($$NODELET{node_id}, 'right', 'top'); %]
    &lt;B&gt;&lt;font [%
      my $attr= $$THEME{nodeletTitleFontAttr};
      return $attr if $attr;
      &quot;color=white&quot;;
      %]&gt;&amp;nbsp;[&quot;$$NODELET{title}&quot;]&lt;/font&gt;&lt;/B&gt;
  &lt;/td&gt;&lt;/tr&gt;
&lt;tr [%
my $bgcol = $$THEME{nodeletBodyColor};
$bgcol ||= &quot;#CCCCCC&quot;;
&quot;bgcolor='$bgcol'&quot;;
%]&gt;&lt;td&gt;&lt;FONT [%
my $fa = $$THEME{nodeletBodyFontAttr};
$fa ||= &quot;size=2&quot;;
$fa;
%]&gt;&lt;ul&gt;
CONTAINED_STUFF
&lt;/ul&gt;
&lt;/font&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr [%
my $bgcol = $$THEME{nodeletDivColor};
$bgcol ||= '#999999';
&quot;bgcolor='$bgcol'&quot;;
%]&gt;&lt;td&gt;&lt;img src=&quot;&quot; width=1 height=1&gt;&lt;/td&gt;&lt;/tr&gt;
&lt;/tr&gt;
&lt;/table&gt;</field>
  <field name="dynamicauthor_permission" type="literal_value">-1</field>
  <field name="dynamicgroup_permission" type="literal_value">-1</field>
  <field name="dynamicguest_permission" type="literal_value">-1</field>
  <field name="dynamicother_permission" type="literal_value">-1</field>
  <field name="group_usergroup" type="literal_value">-1</field>
  <field name="groupaccess" type="literal_value">iiii-</field>
  <field name="guestaccess" type="literal_value">iiii-</field>
  <field name="loc_location" type="noderef" type_nodetype="location,nodetype">container</field>
  <field name="otheraccess" type="literal_value">iiii-</field>
  <field name="parent_container" type="literal_value">0</field>
  <field name="title" type="literal_value">suit nodelet container</field>
  <field name="type_nodetype" type="noderef" type_nodetype="nodetype,nodetype">container</field>
</NODE>
